{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "typeRoots": [
      "./node_modules/@types",
      "./@types"
    ],
    "paths": {
      "@utils/*": [
        "app/utils/*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "baseUrl": ".",
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
}